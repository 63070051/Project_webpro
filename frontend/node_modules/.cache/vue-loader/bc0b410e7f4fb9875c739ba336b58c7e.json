{"remainingRequest":"/Users/tathus/Documents/web pro/WEEK11-2-EXERCISE copy/frontend/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/tathus/Documents/web pro/WEEK11-2-EXERCISE copy/frontend/src/views/blogs/DetailBlog.vue?vue&type=script&lang=js&","dependencies":[{"path":"/Users/tathus/Documents/web pro/WEEK11-2-EXERCISE copy/frontend/src/views/blogs/DetailBlog.vue","mtime":1649422203263},{"path":"/Users/tathus/Documents/web pro/WEEK11-2-EXERCISE copy/frontend/node_modules/cache-loader/dist/cjs.js","mtime":1649416185770},{"path":"/Users/tathus/Documents/web pro/WEEK11-2-EXERCISE copy/frontend/node_modules/babel-loader/lib/index.js","mtime":1649416188319},{"path":"/Users/tathus/Documents/web pro/WEEK11-2-EXERCISE copy/frontend/node_modules/cache-loader/dist/cjs.js","mtime":1649416185770},{"path":"/Users/tathus/Documents/web pro/WEEK11-2-EXERCISE copy/frontend/node_modules/vue-loader/lib/index.js","mtime":1649416188001}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["DetailBlog.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA2GA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"DetailBlog.vue","sourceRoot":"src/views/blogs","sourcesContent":["<template>\n  <div class=\"container is-widescreen\">\n    <section class=\"section\" v-if=\"error\">\n      <div class=\"container is-widescreen\">\n        <div class=\"notification is-danger\">{{ error }}</div>\n      </div>\n    </section>\n    <section class=\"hero\">\n      <div class=\"hero-body\">\n        <p class=\"title\">{{ blog.title }}</p>\n      </div>\n    </section>\n    <section class=\"section\">\n      <div class=\"content\">\n        <div class=\"card has-background-light\">\n          <div class=\"card-image pt-5\">\n            <div class=\"columns\">\n              <div v-for=\"image in images\" :key=\"image.id\" class=\"column\">\n                <figure class=\"image\">\n                  <img\n                    :src=\"'http://localhost:3000'+image.file_path\"\n                    alt=\"Placeholder image\"\n                    style=\"height: 500px; object-fit: cover;\"\n                  />\n                </figure>\n              </div>\n            </div>\n          </div>\n          <div class=\"card-content\">\n            <div class=\"content\">{{ blog.content }}</div>\n            <div class=\"container pb-3\">\n              <p class=\"subtitle\">Comments</p>\n              <div class=\"columns\">\n                <div class=\"column is-8\">\n                  <input type=\"text\" class=\"input\" v-model=\"commTxt\" placeholder=\"Add new comment\" />\n                </div>\n                <div class=\"column is-4\">\n                  <button @click=\"addComment\" class=\"button\">Add comment</button>\n                </div>\n              </div>\n            </div>\n            <div v-for=\"(comment) in comments\" :key=\"comment.id\" class=\"box\">\n              <article class=\"media\">\n                <div class=\"media-left\">\n                  <figure class=\"image is-64x64\">\n                    <img src=\"https://bulma.io/images/placeholders/128x128.png\" alt=\"Image\" />\n                  </figure>\n                </div>\n                <div class=\"media-content\">\n                  <div class=\"content\">\n                    <p v-show=\"comment_id != comment.id\">{{ comment.comment }}</p>\n                    <input v-model=\"commentText\" class=\"input\" type=\"text\" v-show=\"comment_id == comment.id\">\n                    <p class=\"is-size-7\">{{ comment.comment_date }}</p>\n                  </div>\n                  <nav class=\"level\">\n                    <div class=\"level-left\">\n                      <a class=\"level-item\" aria-label=\"like\">\n                        <span class=\"icon is-small pr-3\">\n                          <i class=\"fas fa-heart\" aria-hidden=\"true\"></i>\n                        </span>\n                        <span @click=\"addcommentlike(comment.id)\">\n                          Like ({{comment.like}})\n                        </span>\n                      </a>\n                    </div>\n                    <div class=\"level-right\">\n                      <div class=\"level-item\">\n                        <button class=\"button is-warning\" v-show=\"comment_id != comment.id\" @click=\"editcomment(comment.id, comment.comment)\">\n                          <span>Edit</span>\n                          <span class=\"icon is-small\">\n                            <i class=\"fas fa-edit\"></i>\n                          </span>\n                        </button>\n                        <button class=\"button is-success\" v-show=\"comment_id == comment.id\" @click=\"saveEditComment(comment.id)\">\n                          <span>Save Comment</span>\n                          <span class=\"icon is-small\">\n                            <i class=\"fas fa-edit\"></i>\n                          </span>\n                        </button>\n                      </div>\n                      <div class=\"level-item\">\n                        <button class=\"button is-danger is-outlined\">\n                          <span @click=\"deleteComment(comment.id, blog.id)\">Delete</span>\n                          <span class=\"icon is-small\">\n                            <i class=\"fas fa-times\"></i>\n                          </span>\n                        </button>\n                      </div>\n                    </div>\n                  </nav>\n                </div>\n              </article>\n            </div>\n          </div>\n          <footer class=\"card-footer\">\n            <router-link class=\"card-footer-item\" to=\"/\">To Home Page</router-link>\n            <a class=\"card-footer-item\" @click=\"deleteBlog\">\n              <span>Delete this blog</span>\n            </a>\n          </footer>\n        </div>\n      </div>\n    </section>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  data() {\n    return {\n      blog: {},\n      comments: [],\n      images: [],\n      error: null,\n      commTxt: \"\",\n      editToggle: -1,\n      editCommentMessage: \"\",\n      comment_id : -1,\n      commentText : ''\n    };\n  },\n  mounted() {\n    this.getBlogDetail(this.$route.params.id);\n  },\n  methods: {\n    getBlogDetail(blogId) {\n      axios\n        .get(`http://localhost:3000/blogs/${blogId}`)\n        .then((response) => {\n          this.blog = response.data.blog;\n          this.images = response.data.images;\n          this.comments = response.data.comments;\n        })\n        .catch((error) => {\n          this.error = error.response.data.message;\n        });\n    },\n    addComment() {\n      axios\n        .post(`http://localhost:3000/${this.blog.id}/comments`, {\n          comment: this.commTxt,\n        })\n        .then((response) => {\n          this.commTxt = ''\n          this.comments.push(response.data);\n        })\n        .catch((error) => {\n          this.error = error.response.data.message;\n        });\n    },\n    editcomment(id, comment){\n      console.log(1)\n      this.comment_id = id\n      this.commentText = comment\n      this.check = true\n    },\n    deleteComment(commentId, blogId){\n      let selectedcomment = this.comments.filter(e => e.id === commentId)[0]\n      const result = confirm(`Are you sure you want to delete \\'${selectedcomment.comment}\\'`);\n      if (result){\n        axios\n        .delete(`http://localhost:3000/comments/${commentId}`)\n        .then((response) => {\n          this.$router.push(`#/blogs/detail/${blogId}`)\n        })\n        .catch((error) => {\n          alert(error.response.data.message)\n        });\n      }\n    },\n    saveEditComment(commentId) {\n      axios\n        .put(`http://localhost:3000/comments/${commentId}/${this.commentText}`)\n        .then((response) => {\n          let selectedcomment = this.comments.filter(e => e.id === commentId)[0]\n          selectedcomment.comment = response.data.comment\n          this.commentText = ''\n          this.comment_id = -1\n          console.log(this.comment_id)\n        })\n        .catch((err) => {\n          console.log(err);\n      });\n    },\n    deleteBlog() {\n      const result = confirm(`Are you sure you want to delete \\'${this.blog.title}\\'`);\n      if (result){\n        axios\n        .delete(`http://localhost:3000/blogs/${this.blog.id}`)\n        .then((response) => {\n          this.$router.push('/')\n        })\n        .catch((error) => {\n          alert(error.response.data.message)\n        });\n      }\n    },\n    addcommentlike(id){\n      axios\n        .put(`http://localhost:3000/comments/addlike/${id}`)\n        .then((response) => {\n          let selectedcomment = this.comments.filter(e => e.id === id)[0]\n          selectedcomment.like = response.data.like;\n        })\n        .catch((err) => {\n          console.log(err);\n      });\n    }\n  },\n};\n</script>"]}]}